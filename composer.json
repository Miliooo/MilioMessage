{
  "name": "milio/message",
  "type": "library",
  "description": "Private messaging",
  "keywords": ["user", "user management"],
  "homepage": "https://github.com/Miliooo/MilioMessage",
  "license": "MIT",
  "authors": [
    {
      "name": "Michiel Boeckaert",
      "email": "Boeckaert@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "doctrine/orm": "~2.4",
    "doctrine/common": "~2.4",
    "ramsey/uuid": "~2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~4",
    "webfactory/doctrine-orm-test-infrastructure": "@stable",
    "symfony/framework-bundle": "~2.3"
  },
  "autoload": {
    "psr-4": {
      "Milio\\Message\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Milio\\Message\\Tests\\": "test/"
    }
  }
